"""Organization domain exceptions."""

from .organization_exceptions import (
    # Base exceptions
    OrganizationDomainError,
    
    # User exceptions
    UserError,
    UserNotFoundError,
    DuplicateUserError,
    InvalidUserStatusError,
    InvalidUserRoleError,
    UserStatusTransitionError,
    
    # Tenant exceptions
    TenantError,
    TenantNotFoundError,
    DuplicateTenantError,
    TenantSlugConflictError,
    InvalidTenantStatusError,
    
    # Team exceptions
    TeamError,
    TeamNotFoundError,
    DuplicateTeamError,
    TeamCapacityExceededError,
    InvalidTeamManagerError,
    UserAlreadyInTeamError,
    
    # Authentication exceptions
    AuthenticationError,
    AuthorizationError,
    InvalidCredentialsError,
    TokenExpiredError,
    InvalidTokenError,
    
    # Email exceptions
    InvalidEmailError,
    EmailNotVerifiedError,
    
    # Invitation exceptions
    InvitationError,
    InvalidInvitationTokenError,
    InvitationAlreadyUsedError,
    
    # Validation exceptions
    DomainValidationError,
    BusinessRuleViolationError,
)

